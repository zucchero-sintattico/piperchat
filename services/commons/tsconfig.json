{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["**/*"],
	"exclude": [
		"node_modules",
		"dist",
		"package.json",
		"package-lock.json",
		"tsconfig.json"
	],
	"references": []
}
